# Editor configuration, see http://editorconfig.org
root = true
insert_final_newline=false

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline=false
trim_trailing_whitespace = true


[*.md]
max_line_length = off
trim_trailing_whitespace = true
insert_final_newline=false


[*.php]
insert_final_newline=false
trim_trailing_whitespace = true
